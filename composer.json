{
    "name": "sonata-project/goutte-bundle",
    "type": "symfony-bundle",
    "description": "GoutteBundle, a thin wrapper around Goutte (a simple PHP Web Scraper)",
    "homepage": "https://github.com/sonata-project/SonataGoutteBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas rande",
            "email": "thomas.rabaix@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-curl": "*",
        "symfony/browser-kit": "~2.1",
        "fabpot/goutte": "1.0.*@dev",
        "symfony/dom-crawler": "~2.1"
    },
    "require-dev": {
        "guzzle/plugin-history": ">=3.0.5,<3.8-dev",
        "guzzle/plugin-mock": ">=3.0.5,<3.8-dev"
    },
    "autoload": {
        "psr-0": { "Sonata\\GoutteBundle": "." }
    },
    "target-dir": "Sonata/GoutteBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
